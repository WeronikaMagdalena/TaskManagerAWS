{
  "family": "task-manager-task",
  "executionRoleArn": "arn:aws:iam::454726657221:role/LabRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "task-manager-backend",
      "image": "454726657221.dkr.ecr.us-east-1.amazonaws.com/backend-image-repo:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}